capsule {
  id (example.multichoice_adventure_game) //replace with your capsule id
  version (1.0.0)
  format(3)
  targets {
    target (bixby-mobile-en-US)
  }
  capsule-imports {
    import (viv.core) { 
      as (core) 
    }
  }
  runtime-version (2){
    overrides {
      no-fallback-to-result-collections (true)
    }
  }
  store-countries {
    all
  }
  store-sections {
    section (GamesAndFun)
  }
 }
